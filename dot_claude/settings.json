{
  "env": {
    "BASH_DEFAULT_TIMEOUT_MS": 600000,
    "BASH_MAX_TIMEOUT_MS": 600000,
    "SHELL": "/opt/homebrew/bin/zsh"
  },
  "allowedTools": [
    "Create",
    "Edit",
    "Read",
    "Search",
    "Write",
    "Update",
    "GlobTool",
    "GrepTool",
    "Fetch",
    "WebFetch",
    "WebSearch",
    "Bash"
  ],
  "permissions": {
    "allow": [
      "Edit(**)",
      "Read(**)",
      "Write(**)",
      "GlobTool(**)",
      "GrepTool(**)",
      "WebSearch(domain:*)",
      "Bash(ls:*)",
      "Bash(mv:)",
      "Bash(rg:*)",
      "Bash(fd:*)",
      "Bash(find:*)",
      "Bash(cd:*)",
      "Bash(echo:*)",
      "Bash(pwd:)",
      "Bash(cat:*)",
      "Bash(mkdir:*)",
      "Bash(touch:*)",
      "Bash(grep:*)",
      "Bash(node:*)",
      "Bash(bun:*)",
      "Bash(make:*)",
      "Bash(melos:*)",
      "Bash(mise:*)",
      "Bash(flutter:*)",
      "Bash(dart:*)",
      "Bash(cargo build:*)",
      "Bash(cargo run:*)",
      "Bash(cargo test:*)"
    ],
    "deny": [
      "Bash(rm:)",
      "Bash(git config:*)",
      "Bash(git pull:*)",
      "Bash(git push:*)",
      "Bash(git commit:*)",
      "Bash(git checkout:*)",
      "Bash(git rebase:*)",
      "Bash(git tag:*)",
      "Bash(brew install:*)"
    ]
  },
  "cleanupPeriodDays": 30
}
